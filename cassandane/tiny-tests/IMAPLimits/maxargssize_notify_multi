#!perl
use Cassandane::Tiny;

sub test_maxargssize_notify_multi
{
    my ($self) = @_;

    $self->assert_cmd_bye_toobig('NOTIFY', 'SET',
                                 [ 'PERSONAL', 'NONE' ],
                                 [ 'SELECTED', 'NONE' ],
                                 [ 'SUBSCRIBED', 'NONE' ] );
}
