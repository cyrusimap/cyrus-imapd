#!perl
use Cassandane::Tiny;

sub test_maxargssize_store_annot2
{
    my ($self) = @_;

    $self->assert_cmd_bye_toobig('STORE', '1', 'ANNOTATION',
                                 [ "X" x 5, [ 'VALUE', { Quote => "X" x 25 } ],
                                   "X" x 5, [ 'VALUE', { Quote => "X" x 25 } ] ] );
}
