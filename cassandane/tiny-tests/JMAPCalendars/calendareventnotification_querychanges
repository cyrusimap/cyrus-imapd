#!perl
use Cassandane::Tiny;

sub test_calendareventnotification_querychanges
    :min_version_3_3
    ($self)
{
    my $jmap = $self->{jmap};

    my $res = $jmap->CallMethods([
        ['CalendarEventNotification/queryChanges', {
            sinceQueryState => 'whatever',
        }, 'R1']
    ]);
    $self->assert_str_equals('cannotCalculateChanges', $res->[0][1]{type});
}
