#!perl
use Cassandane::Tiny;

sub test_calendarevent_get_privacy
    :min_version_3_1
    ($self)
{

    my ($id, $ical) = $self->icalfile('privacy');

    my $event = $self->putandget_vevent($id, $ical);
    $self->assert_not_null($event);
    $self->assert_str_equals("private", $event->{privacy});
}
